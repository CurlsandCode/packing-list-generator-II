<nav class="">
    <ul>
        <li> <%= link_to "Welcome Home", welcome_path %> </li>
        <% binding.pry%>
        <% if user_signed_in? %>
          <li><%= "Signed in as #{current_user.email}" %></li>
          <li><%= link_to("Sign out", destroy_user_session_path, :method => 'delete') %></li>

          <% if !!current_user.current_trip %>
            <li> <%= link_to "Trip", trip_path(current_user.current_trip) %></li>
          <% end %>

        <% else %>
          <li><%= link_to("sign in", new_user_session_path) %></li>
          <li><%= link_to("Sign up", new_user_registration_path) %></li>
          <li><%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %></li>
        <% end %>


    </ul>
</nav>
